[Unit]
Description=Mitasny Gunicorn daemon
After=network.target

[Service]
User=django
Group=django
WorkingDirectory=/home/django/sites/mitasny.com/source
ExecStart=/home/django/sites/mitasny.com/virtualenv/bin/gunicorn --bind unix:/tmp/mitasny.socket mitasny.wsgi:application --workers 2
WantedBy=multi-user.target
